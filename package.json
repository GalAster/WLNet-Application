{
    "name": "toml",
    "displayName": "TOML Language Support",
    "description": "TOML language support",
    "version": "0.4.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/GalAster/vscode-toml.git"
    },
    "engines": {
        "vscode": "^1.8.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "scripts": {
        "build": "ts-node source/build.ts && tsc",
        "lint": "tslint **/*.ts --fix"
    },
    "contributes": {
        "languages": [
            {
                "id": "toml",
                "aliases": [
                    "TOML"
                ],
                "extensions": [
                    ".toml",
                    ".tml"
                ],
                "filenames": [
                    "Cargo.lock",
                    "Gopkg.lock"
                ],
                "mimetypes": [
                    "text/x-toml"
                ],
                "configuration": "./extension/toml.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "toml",
                "scopeName": "source.toml",
                "path": "./extension/toml.tmLanguage.json"
            }
        ]
    },
    "dependencies": {
        "toml_vscode": "^0.1.1"
    },
    "devDependencies": {
        "@types/node": "^11.13.6"
    }
}
